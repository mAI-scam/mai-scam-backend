# MAI Scam Detection System - Environment Variables
# Copy this file to .env and update the values

APP_ENV=dev
SEA_LION_API_KEY=your_sealion_api_key_here

# Email Configuration
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USE_TLS=true
SMTP_SENDER_NAME=MAI Scam Detection
REPORT_EMAIL=your_report_email@gmail.com

# Email and Phone Validation Service
VALIDATION_API_KEY=your_validation_api_key

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1

# SageMaker Endpoint
SAGEMAKER_ENDPOINT_NAME=your_sagemaker_endpoint_name